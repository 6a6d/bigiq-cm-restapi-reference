{
    "swagger": "2.0",
    "info": {
        "title": "BIG-IQ Device Trust",
        "description": "API used to create a device trust certificate between BIG-IQ and BIG-IP.",
        "version": "5.2"
    },
    "schemes": [
        "https"
    ],
    "produces": [
        "application/json"
    ],
    "consumes": [
        "application/json"
    ],
    "basePath": "/mgmt/cm/global/tasks",
    "parameters": {
        "paramObjectId": {
            "name": "objectId",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "UUID"
        }
    },
    "definitions": {
        "error_collection": {
            "type": "object",
            "properties": {
                "items": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "description": "Collection of device trust-errors."
                    }
                },
                "kind": {
                    "type": "string",
                    "format": "kind",
                    "description": "Type information for device-trust collections-cm:global:tasks:device-trust:bigiptrusttaskstate.",
                    "readOnly": true
                },
                "message": {
                    "type": "string",
                    "format": "URI",
                    "description": "Error message returned from server.",
                    "readOnly": true
                },
                "requestOperationId": {
                    "type": "integer",
                    "format": "int64",
                    "description": "Unique id assigned to rest operation.",
                    "readOnly": true
                },
                "requestBody": {
                    "type": "string",
                    "format": "request_body",
                    "description": "The data in the request body. GET (None)",
                    "readOnly": true
                },
                "errorStack": {
                    "type": "string",
                    "format": "java_error_stack",
                    "description": "Error stack trace returned by java.",
                    "readOnly": true
                }
            }
        },
        "properties_collection": {
            "type": "object",
            "properties": {
                "items": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "description": "Collection of device trust-properties."
                    }
                },
                "kind": {
                    "type": "string",
                    "format": "kind",
                    "description": "Type information for this device trust collection object.",
                    "readOnly": true
                },
                "selfLink": {
                    "type": "string",
                    "description": "A reference link URI to the device trust collection object.",
                    "format": "URI",
                    "readOnly": true
                },
                "generation": {
                    "type": "integer",
                    "format": "int64",
                    "description": "A integer that will track change made to a device trust collection object. generation.",
                    "readOnly": true
                },
                "lastUpdateMicros": {
                    "type": "integer",
                    "format": "int64",
                    "description": "Update time (micros) for last change made to an device trust collection object. time.",
                    "readOnly": true
                }
            }
        },
        "properties_device-trust": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "format": "UUID",
                    "description": "Unique id assigned to a device trust task object.",
                    "readOnly": true
                },
                "address": {
                    "type": "string",
                    "description": "IP address of device object."
                },
                "userName": {
                    "type": "string",
                    "description": "Username of BIGIQ device object."
                },
                "password": {
                    "type": "string",
                    "description": "Password of device object to be managed."
                },
                "clusterName": {
                    "type": "string",
                    "description": "DSC cluster name of device object to be managed. None if not part of a cluster group."
                },
                "useBigiqSync": {
                    "type": "boolean",
                    "description": "To enable DSC configuration sync. True / False"
                },
                "machineId": {
                    "type": "string",
                    "description": "A unique id string for the BIGIP device."
                },
                "isChassisDevice": {
                    "type": "boolean",
                    "description": "Is this device virtual or appliance. (True / False)"
                },
                "currentStep": {
                    "type": "string",
                    "description": "State machine current step for device trust task."
                },
                "username": {
                    "type": "string",
                    "description": "User name of device object to be managed."
                },
                "status": {
                    "type": "string",
                    "description": "Status of device trust during state transistion."
                },
                "stateDateTime": {
                    "type": "string",
                    "description": "Date/Time when device trust task began. 2016-10-11T10:30:17.834-0400"
                },
                "endDateTime": {
                    "type": "string",
                    "description": "Date/Time when device trust task end. 2016-10-11T10:30:17.834-0400"
                },
                "userReference": {
                    "type": "object",
                    "description": "Reference link to user used to estabish trust. mgmt/shared/authz/users/admin",
                    "properties": {
                        "link": {
                            "type": "string"
                        }
                    }
                },
                "identityReference": {
                    "type": "array",
                    "description": "Array of reference links to user used to estabish trust. mgmt/shared/authz/users/admin",
                    "items": {
                        "type": "object",
                        "properties": {
                            "link": {
                                "type": "string",
                                "format": "URI"
                            }
                        }
                    }
                },
                "kind": {
                    "type": "string",
                    "format": "kind",
                    "description": "Type information for this device trust object.",
                    "readOnly": true
                },
                "selfLink": {
                    "type": "string",
                    "format": "URI",
                    "description": "A reference link URI to the device trust object.",
                    "readOnly": true
                },
                "generation": {
                    "type": "integer",
                    "format": "int64",
                    "description": "A integer that will track change made to a device-trust object. generation.",
                    "readOnly": true
                },
                "lastUpdateMicros": {
                    "type": "integer",
                    "format": "int64",
                    "description": "Update time (micros) for last change made to an policy object. time.",
                    "readOnly": true
                },
                "ownerMachineId": {
                    "type": "string",
                    "description": "A unique id string for the BIGIQ acting as a device owner.",
                    "readOnly": true
                }
            }
        }
    },
    "paths": {
        "/device-trust": {
            "get": {
                "summary": "List all device-trust task items.",
                "description": "Returns the collection of device-trust tasks.",
                "responses": {
                    "200": {
                        "description": "Collection of device-trust tasks.",
                        "schema": {
                            "$ref": "#/definitions/properties_collection"
                        }
                    },
                    "400": {
                        "description": "Error response \"Bad Request\"",
                        "schema": {
                            "$ref": "#/definitions/error_collection"
                        }
                    }
                }
            }
        },
        "/device-trust/{objectId}": {
            "parameters": [
                {
                    "$ref": "#/parameters/paramObjectId"
                }
            ],
            "get": {
                "summary": "Used to get a single device-trust task.",
                "description": "Returns the device-trust identified by id for an endpoint URI.",
                "responses": {
                    "200": {
                        "description": "Device-trust object.",
                        "schema": {
                            "$ref": "#/definitions/properties_device-trust"
                        }
                    },
                    "400": {
                        "description": "Server error response \"Bad Request\".",
                        "schema": {
                            "$ref": "#/definitions/error_collection"
                        }
                    }
                }
            }
        }
    }
}